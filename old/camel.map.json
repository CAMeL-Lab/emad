{
    "format": "(GRP1\\+)?(GRP2\\+)?(GRP3\\+)?(GRP4)(\\.GRP5?GRP6?GRP7?GRP8?)?(\\.GRP9?GRP10?GRP11?GRP12?)?(\\+GRP13)?",
    
    "groups": [
        {
            "name": "PRC2",
            "mapsto": ["prc2"],
            "map": {
                "CONJ": "*_conj",
                "CONJ_SUB": "*_sub",
                "PART": "*_part",
                "PART_CONNECT": "*_conn",
                "PART_RC": "*_rc"
            }
        },
        {
            "name": "PRC1",
            "mapsto": ["prc1"],
            "map": {
                "PREP": "*_prep",
                "PART_FUT": "*_fut",
                "CONJ_SUB": "*_sub",
                "PRON_DEM": "*_dem",
                "PART_EMPHATIC": "*_emph",
                "PART_VOC": "*_voc",
                "PART_PROG": "*_prog",
                "PART_JUS": "*_jus",
                "PRON_INTERROG": "*_interrog"
            }
        },
        {
            "name": "PRC0",
            "mapsto": ["prc2"],
            "map": {
                "PART_DET": "*_det",
                "PART_NEG": "*_neg",
                "PART": "*_part"
            }
        },
        {
            "name": "POS",
            "mapsto": ["pos"],
            "map": {
                "NOUN": "noun",
                "NOUN_NUM": "noun_num",
                "NOUN_PROP": "noun_prop",
                "NOUN_QUANT": "noun_quant",
                "ADJ": "adj",
                "ADJ_NUM": "adj_num",
                "ADJ_COMP": "adj_comp",
                "ADV": "adv",
                "ADV_INTERROG": "adv_interrog",
                "ADV_REL": "adv_rel",
                "VERB": "verb",
                "VERB_PSEUDO": "verb_pseudo",
                "VERB_NOM": "verb",
                "PRON": "pron",
                "PRON_DEM": "pron_dem",
                "PRON_INTERROG": "pron_interrog",
                "PRON_EXCLAM": "pron_exclam",
                "PRON_REL": "pron_rel",
                "PART": "part",
                "PART_DET": "part_det",
                "PART_NEG": "part_neg",
                "PART_FUT": "part_fut",
                "PART_PROG": "part_prog",
                "PART_VERB": "part_verb",
                "PART_VOC": "part_voc",
                "PART_INTERROG": "part_interrog",
                "PART_RESTRICT": "part_restrict",
                "PART_FOCUS": "part_focus",
                "PART_EMPHATIC": "part_emphatic",
                "PART_RC": "part_rc",
                "CONJ_SUB": "conj_sub",
                "PREP": "prep",
                "CONJ": "conj",
                "PART_CONNECT": "part_connect",
                "DIGIT": "digit",
                "ABBREV": "abbrev",
                "INTERJ": "interj",
                "FOREIGN": "latin",
                "PUNC": "punc"
            }
        },
        {
            "name": "Aspect",
            "mapsto": ["asp"],
            "map":{
                "P": "p",
                "I": "i",
                "C": "c"
            }
        },
        {
            "name": "Person",
            "mapsto": ["per"],
            "map":{
                "1": "1",
                "2": "2",
                "3": "3"
            }
        },
        {
            "name": "Gender",
            "mapsto": ["gen"],
            "map":{
                "M": "m",
                "F": "f"
            }
        },
        {
            "name": "Number",
            "mapsto": ["num"],
            "map":{
                "S": "s",
                "D": "d",
                "P": "p"
            }
        },
        {
            "name": "State",
            "mapsto": ["stt"],
            "map":{
                "D": "d",
                "I": "i",
                "C": "c"
            }
        },
        {
            "name": "Case",
            "mapsto": ["cas"],
            "map":{
                "N": "n",
                "G": "g",
                "A": "a"
            }
        },
        {
            "name": "Voice",
            "mapsto": ["vox"],
            "map":{
                "A": "a",
                "P": "p"
            }
        },
        {
            "name": "Mood",
            "mapsto": ["mood"],
            "map":{
                "S": "s",
                "I": "i",
                "J": "j"
            }
        },
        {
            "name": "ENC0",
            "mapsto": ["enc0"],
            "map":{
                "PRON" : ["*_dobj", "*_poss", "*_pron"],
                "PART_NEG" :"part_neg"
            }
        }
    ]
}